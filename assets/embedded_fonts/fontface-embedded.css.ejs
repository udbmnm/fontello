.font-embedded-0 { font-family: 'sola-aoi'; }
.font-embedded-1 { font-family: 'qzone'; }
.font-embedded-2 { font-family: 'tencent'; }
.font-embedded-3 { font-family: 'qq-hd'; }
.font-embedded-4 { font-family: 'social'; }
.font-embedded-5 { font-family: 'entypo'; }
.font-embedded-6 { font-family: 'fontawesome'; }
.font-embedded-7 { font-family: 'iconic'; }
.font-embedded-8 { font-family: 'typicons'; }
.font-embedded-9 { font-family: 'modernpics'; }
.font-embedded-10 { font-family: 'websymbols'; }

@font-face {
  font-family: 'sola-aoi';
  src: url('<%= asset_path("embedded_fonts/sola-aoi.eot") %>');
  src: url('<%= asset_path("embedded_fonts/sola-aoi.eot") %>?#iefix') format('embedded-opentype'),
       url('<%= asset_path("embedded_fonts/sola-aoi.woff") %>') format('woff'),
       url('<%= asset_path("embedded_fonts/sola-aoi.ttf") %>') format('truetype'),
       url('<%= asset_path("embedded_fonts/sola-aoi.svg") %>#sola-aoi') format('svg');
  font-weight: normal;
  font-style: normal;
}

